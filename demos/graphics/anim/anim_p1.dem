mode(1)
t=%pi*(-5:5)/5;
//first plot, to fix boundaries
plot3d1(t,t,sin(t)'*cos(t),35,45," ",[1,2,4]);
storeimage()
for i=35:4:80, // loop on theta angle
  plot3d1(t,t,sin(t)'*cos(t),i,45," ",[1,0,4])
  storeimage();
end
for i=45:3:80, //loop on alpha angle
  plot3d1(t,t,sin(t)'*cos(t),80,i," ",[1,0,4])
  storeimage();
end



