function tt_salso=conv2tex_salso(txt,typdoc)

  rhs=argn(2)
  if rhs<2 then
    typdoc='html'
  end

  tt_salso=[]

  if txt<>[] then

   tt_salso='\begin{itemize}';

   for i=1:size(txt,'r')

     if typdoc=='html' then
       if (txt(i,3)<>"")&(txt(i,3)<>"") then
         tt_salso=[tt_salso;'  '+...
                   '\item{\htmladdnormallink{'+...
                     latexsubst(txt(i,1))+...
                     ' - '+latexsubst(txt(i,3))+...
                     ' ('+latexsubst(txt(i,4))+')'+...
                     '}{'+txt(i,2)+'}}'];
       else
         tt_salso=[tt_salso;'  '+...
                   '\item{\htmladdnormallink{'+...
                     latexsubst(txt(i,1))+...
                     '}{'+txt(i,2)+'}}'];
       end
     else
       //on peut rajouter un \ref{} ici!
       if txt(i,3)<>""&txt(i,3)<>"" then
         tt_salso=[tt_salso;'  '+...
                   '\item{'+...
                     latexsubst(txt(i,1))+...
                     ' - '+latexsubst(txt(i,3))+...
                     ' ('+latexsubst(txt(i,4))+')}'];
       else
         tt_salso=[tt_salso;'  '+...
                   '\item{'+...
                     latexsubst(txt(i,1))+...
                     '}'];
       end
     end
   end
   tt_salso=[tt_salso;'\end{itemize}'];

  end
endfunction